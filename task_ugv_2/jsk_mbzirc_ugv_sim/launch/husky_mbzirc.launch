<launch>

  <arg name="ROBOT_INITIAL_POSE" default="-y 10"/>


  <arg name="laser_enabled" default="true"/>
  <arg name="ur5_enabled" default="true"/>
  <arg name="kinect_enabled" default="false"/>
  <arg name="gripper_enabled" default="true"/>

  <!-- We resume the logic in empty_world.launch, changing only the name of the world to be launched -->
  <include file="$(find husky_gazebo)/launch/husky_empty_world.launch">
    <arg name="world_name" value="$(find jsk_mbzirc_common)/../gazebo_model/world/mbzirc_arena.world"/>
    <arg name="laser_enabled" value="$(arg laser_enabled)"/>
    <arg name="ur5_enabled" value="$(arg ur5_enabled)"/>
    <arg name="kinect_enabled" value="$(arg kinect_enabled)"/>
    <arg name="gripper_enabled" value="$(arg gripper_enabled)"/>
  </include>



</launch>
